<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:dither="true"
            android:shape="oval">
            <solid android:color="@color/white" />
            <stroke
                android:color="@color/black"
                android:width="1dp" />
        </shape>
    </item>

    <item
        android:bottom="20dp"
        android:left="20dp"
        android:right="20dp"
        android:top="20dp" >
        <shape
            android:shape="oval" >
            <solid android:color="@color/black" />
            <size
                android:width="30dp"
                android:height="30dp" />

        </shape >
    </item >

</layer-list>